for i in fcos-teta fcos-master fcos-worker-{1..2}
do
  virsh destroy $i
  virsh undefine $i
done

virsh pool-destroy guests
virsh pool-undefine guests

virsh pool-destroy images
virsh pool-undefine images

rm -f $PWD/var/lib/libvirt/storage/* 
#rm -f $PWD/var/lib/libvirt/images/*

sudo virsh net-destroy otranet
sudo virsh net-undefine otranet
rm -f nat225.xml

rm -rf $PWD/kubespray
